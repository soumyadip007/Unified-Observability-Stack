{
  "title": "Service â€“ Request / HTTP",
  "tags": ["observability", "demo", "otel", "requests"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Request rate by route",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [{
        "expr": "sum(rate(http_server_duration_milliseconds_count[1m])) by (http_route)",
        "legendFormat": "{{http_route}}",
        "refId": "A"
      }],
      "fieldConfig": {"defaults": {"unit": "reqps", "custom": {"drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 10}}},
      "options": {"legend": {"displayMode": "table", "placement": "bottom"}}
    },
    {
      "id": 2,
      "title": "Request rate by HTTP method",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [{
        "expr": "sum(rate(http_server_duration_milliseconds_count[1m])) by (http_method)",
        "legendFormat": "{{http_method}}",
        "refId": "A"
      }],
      "fieldConfig": {"defaults": {"unit": "reqps", "custom": {"drawStyle": "line", "fillOpacity": 10}}},
      "options": {"legend": {"displayMode": "table", "placement": "bottom"}}
    },
    {
      "id": 3,
      "title": "Request rate by status code",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [{
        "expr": "sum(rate(http_server_duration_milliseconds_count[1m])) by (http_status_code)",
        "legendFormat": "{{http_status_code}}",
        "refId": "A"
      }],
      "fieldConfig": {"defaults": {"unit": "reqps", "custom": {"drawStyle": "line", "fillOpacity": 10}}},
      "options": {"legend": {"displayMode": "table", "placement": "bottom"}}
    },
    {
      "id": 4,
      "title": "Total request count (5m)",
      "type": "stat",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [{
        "expr": "sum(increase(http_server_duration_milliseconds_count[5m]))",
        "refId": "A"
      }],
      "fieldConfig": {"defaults": {"unit": "short", "color": {"mode": "palette-classic"}}},
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}}
    }
  ],
  "time": {"from": "now-30m", "to": "now"},
  "templating": {"list": []},
  "annotations": {"list": []},
  "editable": true,
  "links": []
}
