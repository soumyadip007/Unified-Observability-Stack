{
  "title": "Service Overview",
  "tags": ["observability", "demo", "otel"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 2,
  "refresh": "5s",
  "panels": [
    {
      "id": 1,
      "title": "Request Rate by Route",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [{
        "expr": "rate(http_server_duration_milliseconds_count[1m])",
        "legendFormat": "{{http_route}}",
        "refId": "A"
      }],
      "fieldConfig": {"defaults": {"unit": "reqps", "custom": {"drawStyle": "line", "lineInterpolation": "smooth", "fillOpacity": 10}}},
      "options": {"legend": {"displayMode": "table", "placement": "bottom"}}
    },
    {
      "id": 2,
      "title": "Error Rate by Route",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [{
        "expr": "sum(rate(http_server_duration_milliseconds_count{http_status_code=~\"5..\"}[1m])) by (http_route) / sum(rate(http_server_duration_milliseconds_count[1m])) by (http_route) * 100",
        "legendFormat": "{{http_route}}",
        "refId": "A"
      }],
      "fieldConfig": {"defaults": {"unit": "percent", "min": 0, "max": 100, "thresholds": {"mode": "absolute", "steps": [{"value": null, "color": "green"}, {"value": 5, "color": "yellow"}, {"value": 10, "color": "red"}]}, "custom": {"drawStyle": "line", "fillOpacity": 20}}},
      "options": {"legend": {"displayMode": "table", "placement": "bottom"}}
    },
    {
      "id": 3,
      "title": "P99 Latency by Route",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [{
        "expr": "histogram_quantile(0.99, sum(rate(http_server_duration_milliseconds_bucket[1m])) by (le, http_route))",
        "legendFormat": "{{http_route}}",
        "refId": "A"
      }],
      "fieldConfig": {"defaults": {"unit": "ms", "custom": {"drawStyle": "line", "lineInterpolation": "smooth"}}},
      "options": {"legend": {"displayMode": "table", "placement": "bottom"}}
    },
    {
      "id": 5,
      "title": "Overall Service Health",
      "type": "stat",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [{
        "expr": "sum(rate(http_server_duration_milliseconds_count{http_status_code=~\"5..\"}[1m])) / sum(rate(http_server_duration_milliseconds_count[1m])) * 100",
        "refId": "A"
      }],
      "fieldConfig": {"defaults": {"unit": "percent", "min": 0, "max": 100, "thresholds": {"mode": "absolute", "steps": [{"value": null, "color": "green"}, {"value": 5, "color": "red"}]}, "mappings": [], "color": {"mode": "thresholds"}}},
      "options": {"graphMode": "none", "colorMode": "background", "justifyMode": "auto", "orientation": "auto", "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""}, "textMode": "value_and_name"}
    }
  ],
  "time": {"from": "now-15m", "to": "now"},
  "timepicker": {},
  "templating": {"list": []},
  "annotations": {"list": []},
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "weekStart": ""
}
